    "@gatsby-tv/services": "^0.7.0",
    "@gatsby-tv/styles": "^0.7.0",
    "@gatsby-tv/utilities": "^0.7.0",
    "bufferutil": "^4.0.3",
    "chalk": "^4.1.1",
    "cids": "^1.1.7",
    "core-js": "^3.16.1",
    "electron-debug": "^3.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "ipfs": "^0.54.4",
    "it-drain": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
