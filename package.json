{
  "name": "@gatsby-tv/studio",
  "version": "0.1.0",
  "description": "Application for hosting videos on Gatsby.",
  "repository": "https://github.com/gatsby-tv/studio",
  "license": "AGPL-3.0",
  "main": "./dist/main.prod.js",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "build:main": "node scripts/build.main.js",
    "build:renderer": "node scripts/build.renderer.js",
    "start": "node scripts/start.js",
    "start:main": "node scripts/start.main.js",
    "start:renderer": "node scripts/start.renderer.js",
    "postinstall": "node scripts/postinstall.js",
    "package": "node scripts/package.js",
    "format": "node scripts/format.js"
  },
  "build": {
    "productName": "GatsbyStudio",
    "appId": "tv.gatsby.Studio",
    "files": [
      "dist/",
      "node_modules/",
      "index.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "app",
      "output": "release"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/plugin-transform-react-inline-elements": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@babel/runtime": "^7.14.0",
    "@gatsby-tv/types": "^0.4.24",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/sass": "^1.16.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^4.1.1",
    "concurrently": "^6.1.0",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "electron": "^12.0.9",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^2.3.5",
    "file-loader": "^6.2.0",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "swr": "^0.5.6",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "dependencies": {
    "@gatsby-tv/components": "^0.5.0",
    "@gatsby-tv/content": "^0.5.0",
    "@gatsby-tv/icons": "^0.5.0",
    "@gatsby-tv/preview": "^0.5.0",
    "@gatsby-tv/utilities": "^0.5.0",
    "electron-debug": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "source-map-support": "^0.5.19"
  }
}
