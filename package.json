{
  "name": "@gatsby-tv/studio",
  "version": "0.1.0",
  "description": "Application for hosting videos on Gatsby.",
  "repository": "https://github.com/gatsby-tv/studio",
  "license": "AGPL-3.0",
  "main": "./dist/main.prod.js",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "build:main": "node scripts/build.main.js",
    "build:server": "node scripts/build.server.js",
    "build:renderer": "node scripts/build.renderer.js",
    "start": "node scripts/start.js",
    "start:main": "node scripts/start.main.js",
    "start:server": "node scripts/start.server.js",
    "start:renderer": "node scripts/start.renderer.js",
    "postinstall": "node scripts/postinstall.js",
    "prepack": "node scripts/prepack.js",
    "format": "node scripts/format.js",
    "types": "tsc --noEmit"
  },
  "build": {
    "productName": "GatsbyStudio",
    "appId": "tv.gatsby.Studio",
    "files": [
      "dist/",
      "node_modules/",
      "index.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "app",
      "output": "release"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/plugin-transform-react-inline-elements": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@babel/runtime": "^7.14.0",
    "@gatsby-tv/types": "^0.5.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node-ipc": "^9.1.5",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/sass": "^1.16.0",
    "babel-loader": "^8.2.2",
